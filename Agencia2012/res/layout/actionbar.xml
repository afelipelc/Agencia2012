<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
       android:layout_width="match_parent"
        android:layout_height="72dp"
        android:orientation="vertical" android:background="@drawable/fondobar" android:paddingTop="0dp" android:paddingBottom="0dp">

        <LinearLayout
            android:id="@+id/linearLayout4"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/car_shop" android:paddingLeft="8dp" android:paddingTop="5dp"/>

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Agencia 2012" android:padding="8dp" android:textColor="#0c0c0c"/>

        </LinearLayout>


        <LinearLayout
            android:id="@+id/linearLayout1"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:padding="2dp" >


            <Button
                android:id="@+id/AtrasBtn"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:background="@drawable/redondeado"
                android:drawablePadding="4dp"
                android:text="Atras" android:textSize="14sp" android:textColor="@color/textoboton" android:paddingTop="0dp" android:paddingBottom="0dp" android:layout_weight="0.1"/>



            <TextView
                android:id="@+id/TituloNavbar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="center"
                android:paddingLeft="8dp"
                android:paddingRight="6dp"
                android:text="TÃ­tulo"
                android:textColor="#000000"
                android:textSize="16sp" />

            <LinearLayout
                android:id="@+id/linearLayout3"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="right" android:layout_weight="1">


                <ImageButton
                    android:id="@+id/clientesnavbtn"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/redondeado" android:paddingLeft="2dp" android:paddingRight="2dp" android:src="@drawable/client_small2"/>

                <LinearLayout
                    android:id="@+id/linearLayout2"
                    android:layout_width="2dp"
                    android:layout_height="match_parent" >
                </LinearLayout>


                <ImageButton
                    android:id="@+id/autosnavbtn"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/redondeado"
                    android:paddingLeft="2dp" android:paddingRight="2dp" android:src="@drawable/car_small"/>

                <LinearLayout
                    android:id="@+id/LinearLayout01"
                    android:layout_width="2dp"
                    android:layout_height="match_parent" >
                </LinearLayout>


                <ImageButton
                    android:id="@+id/ventasnavbtn"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/redondeado"
                    android:paddingLeft="2dp" android:paddingRight="2dp" android:src="@drawable/report_small"/>

                <LinearLayout
                    android:id="@+id/LinearLayout02"
                    android:layout_width="2dp"
                    android:layout_height="match_parent" >
                </LinearLayout>


                <ImageButton
                    android:id="@+id/opcionesnavbtn"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/redondeado"
                    android:src="@drawable/options_small" android:paddingLeft="2dp" android:paddingRight="2dp"/>

            </LinearLayout>

    </LinearLayout>
</LinearLayout>
